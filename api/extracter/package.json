{
  "name": "optc-db-api",
  "version": "1.0.0",
  "private": true,
  "scripts": {
    "old-extract": "ts-node old-extract.ts",
    "old-schema": "ts-json-schema-generator -f ./tsconfig.json -o './old-to-raw/models/old-db-schema.json' --path './old-to-raw/models/old-db.d.ts' --type 'OldDB.ExtendedUnit'",
    "extract": "ts-node extract.ts",
    "raw-schema": "ts-json-schema-generator -f ./tsconfig.json --path './raw-db/models/raw-db.d.ts' -o './raw-db/models/raw-db-schema.json' --type 'RawDB.Character'"
  },
  "dependencies": {
    "@types/node": "^17.0.31",
    "@types/turndown": "^5.0.1",
    "@typescript-eslint/eslint-plugin": "^5.22.0",
    "ajv": "^8.11.0",
    "eslint-config-standard": "^17.0.0",
    "eslint-plugin-standard": "^5.0.0",
    "ts-json-schema-generator": "^1.0.0",
    "ts-node": "^10.7.0",
    "turndown": "^7.1.1",
    "typescript": "^4.6.4",
    "yaml": "^2.0.1"
  }
}
